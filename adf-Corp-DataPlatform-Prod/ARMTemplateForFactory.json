{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-Corp-DataPlatform-Prod"
		},
		"bdConsolidado_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bdConsolidado'"
		},
		"blob_Cargas_DataFactory_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_Cargas_DataFactory'"
		},
		"azsql_bdConsolidado_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsql_bdConsolidado'"
		},
		"kv_Credentials_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Corp-Credentials.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/table_Unidades')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "azsql_bdConsolidado",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "CNPJ",
						"type": "varchar"
					},
					{
						"name": "RAZAO_SOCIAL",
						"type": "varchar"
					},
					{
						"name": "ENDERECO",
						"type": "varchar"
					},
					{
						"name": "BAIRRO",
						"type": "varchar"
					},
					{
						"name": "CIDADE",
						"type": "varchar"
					},
					{
						"name": "CEP",
						"type": "varchar"
					},
					{
						"name": "HIERARQUIA",
						"type": "varchar"
					},
					{
						"name": "DATA_CARGA",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "tbUNIDADES"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/azsql_bdConsolidado')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/kv_Credentials')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('kv_Credentials_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/bdConsolidado')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('bdConsolidado_connectionString')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "kv_Credentials",
							"type": "LinkedServiceReference"
						},
						"secretName": "AdmSQL-Credential"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/kv_Credentials')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/blob_Cargas_DataFactory')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('blob_Cargas_DataFactory_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/azsql_bdConsolidado')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('azsql_bdConsolidado_connectionString')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "kv_Credentials",
							"type": "LinkedServiceReference"
						},
						"secretName": "AdmSQL-Credential"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/kv_Credentials')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ir-Gateway-DataPlatform-Prod')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}