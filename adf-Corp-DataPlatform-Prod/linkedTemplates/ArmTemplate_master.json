{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-Corp-DataPlatform-Prod"
		},
		"bdConsolidado_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bdConsolidado'"
		},
		"blob_Cargas_DataFactory_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_Cargas_DataFactory'"
		},
		"azsql_bdConsolidado_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsql_bdConsolidado'"
		},
		"kv_Credentials_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Corp-Credentials.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_Cargas_DataFactory_connectionString": {
						"value": "[parameters('blob_Cargas_DataFactory_connectionString')]"
					},
					"bdConsolidado_connectionString": {
						"value": "[parameters('bdConsolidado_connectionString')]"
					},
					"azsql_bdConsolidado_connectionString": {
						"value": "[parameters('azsql_bdConsolidado_connectionString')]"
					},
					"kv_Credentials_properties_typeProperties_baseUrl": {
						"value": "[parameters('kv_Credentials_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}